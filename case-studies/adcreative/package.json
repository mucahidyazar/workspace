  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "commit": "cz",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|gql|graphql|mdx|vue)\"",
    "prepare": "husky install",
    "build:analyze": "ANALYZE=true next build",
    "up:dev": "docker-compose -f docker-compose.dev.yml up",
    "up:prod": "docker-compose -f docker-compose.prod.yml up",
    "generate:sitemap": "next-sitemap --config src/configs/next-sitemap.js",
    "generate:rss": "ts-node src/configs/generate-rss.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@next/bundle-analyzer": "^13.0.5",
    "@next/font": "13.1.1",
    "@tanstack/react-query": "^4.18.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "axios": "^1.2.1",
    "commitizen": "^4.2.5",
    "cookies-next": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "date-fns": "^2.29.3",
    "eslint": "8.30.0",
    "eslint-config-next": "13.1.1",
    "husky": "^8.0.2",
    "mucahid-test1": "0.0.3",
    "mucahid-test2": "0.0.1",
    "mucahid-test3": "0.0.1",
    "next-translation": "1.0.0",
    "nanoid": "^4.0.0",
    "next": "^13.0.7",
    "next-sitemap": "^3.1.32",
    "react": "18.2.0",
    "react-content-loader": "^6.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.39.7",
    "react-icons": "^4.7.1",
    "react-toastify": "^9.1.1",
    "styled-components": "^5.3.6",
    "typescript": "4.9.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/react-syntax-highlighter": "^15.5.5",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "autoprefixer": "^10.4.13",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-exports": "^0.8.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.8.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
