    "react-dom": "^18.0.0",
    "react-redux": "^8.0.0",
    "react-router-dom": "6",
    "react-select": "^5.3.0",
    "react-toastify": "^8.2.0",
    "redux-devtools-extension": "^2.13.9",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
